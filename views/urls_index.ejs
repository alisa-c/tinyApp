<!DOCTYPE html>

<html lang="en">

<head>
  <title>TinyApp</title>
</head>

<header>
  <% include ./partials/_header %>
</header>
  <body>
    <ul>
      <% for(const url in urls){ %>
        <li>
          <%= url %> --->
            <a href='/u/<%= url %>'><%= urls[url] %></a>
              <form method="GET" action="/urls/<%= url %>">
                <input type="submit" value="Edit">
              </form>
              <form method="POST" action="/urls/<%= url %>/delete">
                <input type="submit" value="Delete">
              </form>
        </li>
      <%  };  %>
    </ul>
    <form action="/urls/new" method="GET">
      <input type="submit" value="Add more urls">
    </form>
  </body>

</html>

